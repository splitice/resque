{
    "name": "php-resque/resque",
    "type": "library",
    "description": "Redis backed library for creating background jobs and processing them later. Based on Ruby resque.",
    "keywords": ["job", "background", "redis", "resque", "worker"],
    "homepage": "http://www.github.com/php-resque/resque/",
    "license": "MIT",
    "authors": [
        {
            "name": "Paul Croker",
            "email": "paul@zomble.com.au"
        },
        {
            "name": "Chris Boulton",
            "email": "chris@bigcommerce.com"
        }
    ],
    "require": {
        "php": ">=5.4",
        "ext-posix": "*",
        "ext-pcntl": "*",
        "predis/predis": "~1.0",
        "psr/log": "~1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.1",
        "phpspec/phpspec": "~2.0"
    },
    "suggest": {
        "ext-proctitle": "Optionally, allows php-resque to rename the title of UNIX processes to show the status of a worker."
    },
    "bin": [
        "bin/resque"
    ],
    "autoload": {
        "psr-0": {
            "Resque": "src"
        }
    }
}
